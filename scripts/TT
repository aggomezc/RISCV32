    %store/vec4 v0x55da98354830_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x55da98354760_0, 0, 32;
    %delay 1, 0;
    %load/vec4 v0x55da98354830_0;
    %inv;
    %store/vec4 v0x55da98354830_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0x55da98354830_0;
    %inv;
    %store/vec4 v0x55da98354830_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x55da98354760_0, 0, 32;
    %delay 1, 0;
    %load/vec4 v0x55da98354830_0;
    %inv;
    %store/vec4 v0x55da98354830_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0x55da98354830_0;
    %inv;
    %store/vec4 v0x55da98354830_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x55da98354760_0, 0, 32;
    %delay 1, 0;
    %load/vec4 v0x55da98354830_0;
    %inv;
    %store/vec4 v0x55da98354830_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0x55da98354830_0;
    %inv;
    %store/vec4 v0x55da98354830_0, 0, 1;

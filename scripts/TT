T_0 ;
    %vpi_call 3 11 "$readmemh", "INSTRS/HEX_FILE_CORRECTO.hex", v0x55da98354530 {0 0 0};
    %pushi/vec4 2147483647, 0, 32;
    %store/vec4 v0x55da98308ba0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x55da98306df0;
T_1 ;
    %wait E_0x55da98341960;
    %ix/getv 4, v0x55da98354470_0;
    %load/vec4a v0x55da98354530, 4;
    %assign/vec4 v0x55da98308ba0_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55da9832d090;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "MEM_TEST.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55da9832d090 {0 0 0};
